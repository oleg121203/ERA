{
  "name": "snake-game-monorepo",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "client",
    "server"
  ],
  "scripts": {
    "start:client": "cd client && npm start",
    "start:server": "cd server && npm start",
    "dev": "concurrently \"npm run start:client\" \"npm run start:server\"",
    "test": "npm run test:client && npm run test:server"
  },
  "devDependencies": {
    "concurrently": "^8.0.0"
  }
}
